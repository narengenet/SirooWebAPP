@page
@model SirooWebAPP.UI.Pages.Superadmins.ExportChallengeUserDataModel
@{
    Layout = "_LayoutClients";
}
<style>
    table td {
        width: 20%;
        text-align: center;
        font-family: 'Yekan';
    }

    table tr {
        border: 1px solid white;
    }

    .MainDashboard {
        display: none !important;
    }

</style>




<div class="row justify-content-center mb-3">
    <div class="col-12 siroo-header">تعداد همه رکوردها: @Model.allChallengeUserData </div>
</div>

<div class="row col-12 mb-3 justify-content-center">
    <div class="col-8 SirooBtn">
        <input type="submit" class="col-12" onclick="ExportChallengeData();" value="خروجی کلیه ثبت نام ها">
    </div>
</div>


<div class="row">&nbsp;</div>
<div class="row justify-content-center mb-3">
    <div class="col-12 siroo-header">تعداد رکوردهای جدید: @Model.newChallengeUserData </div>
</div>


<div class="row col-12 mb-3 justify-content-center">
    <div class="col-8 SirooBtn">
        <input type="submit" class="col-12" onclick="ExportNewChallengeData();" value="خروجی ثبت نام های جدید">
    </div>
</div>




@section Scripts {
<script src="~/js/pages/ExportChallengeUserData-default.js?i=1"></script>

}